{
  "name": "rtl-sbi-performance",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest --verbose",
    "preview": "vite preview",
    "storybook": "npm run generate && storybook dev -p 6006",
    "generate": "npm run generate-storybook-stories && npm run generate-rtl-testcases",
    "generate-storybook-stories": "mustache ./src/TaskList.json ./src/TaskList.stories.ts.mustache > ./src/TaskList.stories.ts",
    "generate-rtl-testcases": "mustache ./src/TaskList.json ./src/TaskList.test.tsx.mustache > ./src/TaskList.test.tsx",
    "build-storybook": "export NODE_OPTIONS=--openssl-legacy-provider && TEST_RUNNER=true storybook build",
    "test-storybook": "test-storybook --url http://127.0.0.1:6006",
    "host-storybook": "http-server storybook-static --port 6006 --silent"
  },
  "dependencies": {
    "@storybook/jest": "^0.2.1",
    "@storybook/test-runner": "^0.13.0",
    "babel-preset-react-app": "10.0.1",
    "identity-obj-proxy": "3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.41.5",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@storybook/addon-actions": "7.4.0",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "29.3.1",
    "babel-loader": "8.3.0",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "mustache": "^4.2.0",
    "storybook": "^7.4.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "wait-on": "^7.0.1"
  }
}
